public class Bitwise2 {

        public static void main(String[] args) {
                // TODO Auto-generated method stub
                int x= 8;
                int y = 10;
                System.out.println( x<<2);
                System.out.println( y>>2);

                int z=132;
                int T=75;

                System.out.println(z >> 5);
                System.out.println(T <<2);


        }

}
